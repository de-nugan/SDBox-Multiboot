.Key dummy 
echo "This will enable the memory-resident SD-BOX boot device (BOOTRAD) on subsequent boots."
ask "Continue? (Y/N): "

if not WARN
  echo "OK!"
  quit
endif

if not exists ENV
  makedir ENV
endif

echo >ENV/BOOTRAD "TRUE"

echo "BOOTRAD will be installed on next boot. Have a nice day."
wait 3
