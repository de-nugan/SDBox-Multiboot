.Key dummy
echo "This will deactivate the BOOTRAD device."
ask "Continue? (Y/N): "

if not WARN
  echo "OK!"
  quit
endif

if exists ENV/BOOTRAD
  delete ENV/BOOTRAD
endif

failat 21 

assign >NIL: BOOTRAD: exists

if not WARN
  remrad >NIL: BOOTRAD: force
  assign >NIL: RAD: remove
endif

echo "BOOTRAD disabled. Have a nice day."
wait 3
